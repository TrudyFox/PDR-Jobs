@using PDR_Jobs
@using System

<div class="alert alert-secondary mt-4" role="alert">
    <span class="oi oi-pencil mr-2" aria-hidden="true"></span>
    @if (SomeOtherShit != null)
    {
        <strong>@SomeOtherShit.FullName</strong>
        <br />

    }
    <span class="text-nowrap">
        @if (SomeOtherShit.Address != null)
        {
            @SomeOtherShit.Address
        }
    </span>
</div>
@code {

    [Parameter]
    public Tech SomeOtherShit { get; set; }

}